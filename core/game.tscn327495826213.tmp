[sub_resource type="GDScript" id="GDScript_yf72h"]
script/source = "extends Script

static func click(card:Card,card_resource:Resource = card.card_resource) ->void:
	if not card_resource.spawn_cards.is_empty():
		var new_card:Card
		var new_resource:CardResource
		var conveyor:Card
		var belt:Card
		var belt2:Card
		var mixer:Card
		var elf:Card
		
		for spawn:CardResource in card_resource.spawn_cards:
			new_resource = spawn
			if card_resource.card_name in Global.land_cards:
				var above:Card = card.head.stack.get_above(card)
				if above:
					if above.card_resource.card_name in Global.crop_cards:
						new_resource = above.card_resource
					elif above.card_resource.card_name in Global.spawn_crop_cards:
						
						new_resource = above.card_resource.spawn_cards[0]
			
			new_card = Global.create_card(new_resource,card.get_board())
			new_card.global_position = card.global_position
			
			if new_card.card_resource.card_name == \"CONVEYOR\":
				conveyor = new_card
			if new_card.card_resource.card_name == \"BELT\":
				if belt:
					belt2 = new_card
				else:
					belt = new_card
			if new_card.card_resource.card_name == \"MIXER\":
				mixer = new_card
			if new_card.card_resource.card_name == \"ELF\":
				elf = new_card
			
			card.head.stack.add_to_stack(new_card)
			
			var double_resource_chance:float = Global.game.altar_stats_dict[\"double_resource_chance\"][0]
			if new_resource.card_name in Global.resource_cards:
				var chance:float = randf_range(0.0,100.0)
				if chance < double_resource_chance:
					new_card = Global.create_card(new_resource)
					card.head.stack.add_to_stack(new_card)
		
		if conveyor:
			set_up_conveyor(conveyor,belt)
		if elf:
			set_up_elf(elf,belt)
		if mixer:
			set_up_mixer(mixer,belt,belt2)
	if not card_resource.spawn_recipes.is_empty():
		var new_recipe:CardRecipe
		
		for recipe_resource:CardResource in card_resource.spawn_recipes:
			new_recipe = Global.create_recipe(recipe_resource,card.get_board())
			card.head.stack.add_to_stack(new_recipe)
			
	if not card_resource.spawn_letters.is_empty():
		var new_letter:CardRecipe
		
		for letter_resource:Letter in card_resource.spawn_letters:
			new_letter = Global.create_letter(letter_resource,card.get_board())
			card.head.stack.add_to_stack(new_letter)
	
		
	if card.card_resource.card_name == \"GUIDE_1\":
		card.clicker.progress.queue_free()
		card.clicker.queue_free()
		card.title_label.text = \"[center]GUIDE 1\"
		
	if card.card_resource.one_shot:
		card.head.stack.pull_card(card)
		card.queue_release()

static func set_up_conveyor(conveyor:Card, belt:Card) ->void:
	var tether:Tether = load(\"res://cards/components/tether.tscn\").instantiate()
	tether.linked_cards = [conveyor,belt]
	conveyor.add_child(tether)
	conveyor.linked_with_cards = [belt]
	belt.linked_to_cards = [conveyor]

static func set_up_elf(elf:Card, porter:Card) ->void:
	var tether:Tether = load(\"res://cards/components/tether.tscn\").instantiate()
	tether.linked_cards = [elf,porter]
	elf.add_child(tether)
	elf.linked_with_cards = [porter]
	porter.linked_to_cards = [elf]
	porter.card_resource = porter.card_resource.duplicate()
	porter.default_text = \"[center]PORTER[/center]\"
	porter.title_label.text = porter.default_text
	porter.texture.self_modulate = elf.card_resource.color

static func set_up_mixer(mixer:Card, belt:Card, belt2:Card) ->void:
	var tether:Tether = load(\"res://cards/components/tether.tscn\").instantiate()
	var tether2:Tether = tether.duplicate()
	tether.linked_cards = [mixer,belt]
	tether2.linked_cards = [mixer,belt2]

	mixer.add_child(tether)
	mixer.add_child(tether2)
	
	mixer.linked_with_cards = [belt,belt2]
	belt.linked_to_cards = [mixer]
	belt2.linked_to_cards = [mixer]
	
"

